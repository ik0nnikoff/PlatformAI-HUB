{
  "id": "test-agent-123",
  "name": "Test Agent",
  "description": "Test agent for verifying new configuration structure",
  "ownerId": "user-456",
  "config": {
    "simple": {
      "settings": {
        "model": {
          "provider": "openai",
          "model_id": "gpt-4o-mini",
          "temperature": 0.7,
          "system_prompt": "You are a helpful AI assistant.",
          "limit_to_kb": false,
          "answer_in_user_lang": true,
          "use_markdown": true
        },
        "streaming": true,
        "useContextMemory": true,
        "tools": [
          {
            "id": "kb_search",
            "name": "Knowledge Base Search",
            "type": "knowledgeBase",
            "enabled": true,
            "description": "Search through knowledge base documents",
            "settings": {
              "knowledgeBaseIds": ["kb-001", "kb-002"],
              "retrievalLimit": 5,
              "rewriteAttempts": 3,
              "returnToAgent": true,
              "rewriteQuery": true
            }
          },
          {
            "id": "web_search",
            "name": "Web Search",
            "type": "webSearch",
            "enabled": true,
            "description": "Search the web for current information",
            "settings": {
              "searchLimit": 3,
              "include_domains": [],
              "excludeDomains": ["example.com"]
            }
          },
          {
            "id": "api_tool_test",
            "name": "Test API Tool",
            "type": "apiRequest",
            "enabled": true,
            "description": "Test API request with new headers and method support",
            "settings": {
              "apiUrl": "https://httpbin.org/get",
              "method": "GET",
              "headers": [
                {"key": "Authorization", "value": "Bearer test-token"},
                {"key": "Content-Type", "value": "application/json"},
                {"key": "User-Agent", "value": "PlatformAI-Agent/1.0"}
              ],
              "params": [
                {
                  "key": "user_id",
                  "value": "{user_id}"
                },
                {
                  "key": "phone",
                  "value": "{phone_number}"
                }
              ]
            }
          }
        ],
        "integrations": [
          {
            "name": "Telegram",
            "type": "telegram",
            "enabled": true,
            "settings": {
              "bot_token": "test_token"
            }
          }
        ]
      }
    }
  }
}
